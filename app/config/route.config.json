{
  "routes": [
    {
      "route": "/v1/feature",
      "method": "GET",
      "controller": "../controllers/v1/feature/feature-list-controller"
    },
    {
      "route": "/v1/feature/:feature",
      "method": "GET",
      "controller": "../controllers/v1/feature/feature-controller"
    },
    {
      "route": "/v1/browser/stable",
      "method": "GET",
      "controller": "../controllers/v1/browsers/stable-controller"
    },
    {
      "route": "/v1/browser/:browser",
      "method": "GET",
      "controller": "../controllers/v1/browsers/browser-controller"
    }
  ]
}